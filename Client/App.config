<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_IAuthManager">
                    <security>
                        <transport sslProtocols="None" />
                    </security>
                </binding>
                <binding name="NetTcpBinding_IGameChat">
                    <security>
                        <transport sslProtocols="None" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8090/" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IAuthManager" contract="AuthManagerService.IAuthManager"
                name="NetTcpBinding_IAuthManager">
                <identity>
                    <userPrincipalName value="BLOO\aiwass" />
                </identity>
            </endpoint>
            <endpoint address="net.tcp://localhost:8091/" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IGameChat" contract="GameChatService.IGameChat"
                name="NetTcpBinding_IGameChat">
                <identity>
                    <userPrincipalName value="BLOO\aiwass" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>